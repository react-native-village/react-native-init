    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "https-browserify": "~0.0.0",
    "lottie-ios": "3.4.1",
    "lottie-react-native": "^5.1.4",
    "metro-minify-terser": "^0.73.6",
    "patch-package": "^6.5.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-app-auth": "^6.4.3",
    "react-native-crypto": "2.1.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-os": "https://github.com/aprock/react-native-os",
    "react-native-randombytes": "^3.0.0",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.6.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.18.10",
    "readable-stream": "^4.3.0",
    "stream": "^0.0.2",
    "stream-browserify": "^1.0.0",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/typescript-operations": "^2.5.11",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@storybook/addon-actions": "6.3",
    "@storybook/addon-controls": "6.3",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.5.14",
